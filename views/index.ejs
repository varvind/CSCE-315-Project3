<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Election Tracker</title>
</head>
<body>
  <h2>Hello World</h2>
  <% tweets.forEach(tweet => { %>
    <h4><%=tweet.text%></h4>
    <% if(tweet.imgs.length > 0) {%>
      <% tweet.imgs.forEach(img => { %> 
        <iframe src=<%=tweet.img%> title="W3Schools Free Online Web Tutorials"></iframe>
      <% }) %>
    <% } %>
  <%}); %>
</body>
</html>